<div class="page-header">
  <h1>
    Channel associations
    <a href="/channel_associations/new" class="btn btn-lg btn-success">New Channel association</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Data card</th>
        <th>Channel</th>
        <th>Actions</th>
      </tr>

      <% @channel_associations.each do |channel_association| %>
      <tr>
        <td><%= channel_association.data_card_id %></td>
        <td><%= channel_association.channel_id %></td>
        <td>
          <a href="/channel_associations/<%= channel_association.id %>" class="btn btn-primary">Show</a>
          <a href="/channel_associations/<%= channel_association.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_channel_association/<%= channel_association.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
