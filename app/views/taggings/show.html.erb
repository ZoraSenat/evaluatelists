<div class="page-header">
  <h3>Tagging #<%= @tagging.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Tag</dt>
      <dd><%= @tagging.tag_id %></dd>

      <dt>Data card</dt>
      <dd><%= @tagging.data_card_id %></dd>

      <dt>User</dt>
      <dd><%= @tagging.user_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/taggings" class="btn btn-primary">
        Back
      </a>
      <a href="/taggings/<%= @tagging.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_tagging/<%= @tagging.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>

<!-- A Tagging has many user -->

<div class="row">
  <div class="col-md-12">
    <ul class="list-group">
      <li class="list-group-item list-group-item-info">
        User
      </li>

      <% @tagging.user.each do |user| %>
        <li class="list-group-item">
          <a href="/users/<%= user.id %>">
            <%= user.email %>
          </a>
          <div class="btn-group btn-group-xs pull-right">
            <a href="/users/<%= user.id %>" class="btn btn-primary">
              <i class="fa fa-search-plus"></i>
            </a>
            <a href="/users/<%= user.id %>/edit" class="btn btn-warning">
              <i class="fa fa-edit"></i>
            </a>
            <a href="/delete_user/<%= user.id %>" class="btn btn-danger" rel="nofollow">
              <i class="fa fa-trash-o"></i>
            </a>
          </div>
        </li>
      <% end %>
    </ul>
  </div>
</div>
