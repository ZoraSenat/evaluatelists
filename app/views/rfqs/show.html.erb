<div class="page-header">
  <h3>Rfq #<%= @rfq.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>User</dt>
      <dd><%= @rfq.user_id %></dd>

      <dt>Data card</dt>
      <dd><%= @rfq.data_card_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/rfqs" class="btn btn-primary">
        Back
      </a>
      <a href="/rfqs/<%= @rfq.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_rfq/<%= @rfq.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
